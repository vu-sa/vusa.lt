{
  "name": "vusa.lt",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest --project unit --project component",
    "test:all": "vitest",
    "test:unit": "vitest --project unit",
    "test:component": "vitest --project component",
    "test:watch": "vitest --project unit --project component",
    "test:ci": "vitest run --project unit --project component",
    "test:storybook": "vitest --project storybook",
    "coverage": "vitest run --coverage",
    "coverage:ci": "vitest run --coverage --project unit --project component --reporter=default --reporter=junit --outputFile=test-results.junit.xml --coverage.reporter=text-summary --coverage.reporter=lcov --coverage.reporter=clover",
    "coverage:ui": "vitest run --coverage --ui",
    "docs:dev": "vitepress dev docs --port 8081",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 8086",
    "storybook": "STORYBOOK=true storybook dev -p 6006 --no-open",
    "storybook:build": "STORYBOOK=true storybook build",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint resources/js",
    "lint:fix": "eslint resources/js --fix",
    "format": "eslint resources/js --fix",
    "check": "npm run typecheck && npm run lint",
    "generate:pwa": "node dev/generate-pwa-assets.mjs"
